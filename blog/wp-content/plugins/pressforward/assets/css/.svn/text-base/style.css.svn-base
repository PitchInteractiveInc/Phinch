.pfmodal {
	background-color: #f5f5f5;
	max-height: 100%;
	position: fixed;
	top: 0;
	right: 0;
	bottom: 100%;
	left: 0;
	margin: 0;
	width: 100%;
	height: 100%;
	overflow: hidden;
}

.pfmodal .modal-header {
	background-color: white;
	max-height: 13%;

}

.pfmodal .modal-mobile-nav {
	margin: 0 8px;
}

.pfmodal .mobile-goPrev, .pfmodal .mobile-goNext {

}

.pfmodal .modal-body-row {
	background:white;
}

.pfmodal .modal-body {
	min-height: 100%
}

.pfmodal .modal-body-row {
	height:80%;
	overflow:hidden;
}

.pfmodal .modal-sidebar {
	height:100%;
	margin: 0;
	/**width: 25.64102564102563%;**/
	width:25.5%
}

.pfmodal .modal-side-item {
	border: 1px solid #eee;
	border-right: transparent;
	padding: 1%;
	margin: 0;
}

.pfmodal .modal-comments {
	overflow-y: auto;
}

.pfmodal .goPrev, .pfmodal .goNext {
	height: 25%;
	font-size: .7rem;
	line-height: .7rem;
	overflow:hidden;
	text-align: right;
}

.pfmodal .goPrev p, .pfmodal .goNext p {
	margin-bottom: 8px;
}

.pfmodal .goPrev h5, .pfmodal .goNext h5 {
	margin-bottom: 8px;
}

.pfmodal .modal-comments {
	height: 50%;
}

.pfmodal .modal-footer {
	max-height: 80%;
	min-height: 48px;
}

