.error.fade {
	display:none;
}

.toplevel_page_pf-menu #wpcontent {
	margin-left: 160px;
}

.deck .card {

	background-color: #AFC459;
	border-color: #4C5B2C;
	border-radius: 10px;
	display:block;
    width: 100%;
    min-height: 80px;
	height: 100%;
    padding: 6px;

background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#AFC459), to(#84924B));
background-image: -webkit-linear-gradient(left, #AFC459, #84924B);
background-image: -o-linear-gradient(left, #AFC459, #84924B);
background-image: linear-gradient(to right, #AFC459, #84924B);
background-image: -moz-linear-gradient(left, #AFC459, #84924B);
background-repeat: repeat-x;

border-color: #9FB351 #768341 #BFD662 #9FB351;
filter: progid:dximagetransform.microsoft.gradient(startColorstr='#AFC459', endColorstr='#768341', GradientType=1);
filter: progid:dximagetransform.microsoft.gradient(enabled=false);	

}

.deck .card:hover {
	background-color: #84924B;
}

.deck .card:active {
	background-color: #9FB351;
}

.tapped {
	font-size: 13px;
	font-weight: bold;
	-webkit-transform: rotate(180deg);
	-webkit-writing-mode: vertical-rl;
	-moz-transform: rotate(180deg);
	-moz-writing-mode: vertical-rl;
	-o-transform: rotate(180deg);
	-o-writing-mode: vertical-rl;
	filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=2);
	-ms-transform: rotate(180deg);
	-ms-writing-mode: vertical-rl;	
	writing-mode: tb-rl;
	white-space: nowrap;
	line-height: 0;
	margin: 0 auto;
	padding-left: 4px;
	padding-top: 6px;
	padding-bottom: 6px;
}

.tapped:after
{
content:".";
display:block;
clear:both;
visibility:hidden;
}

.main-container {
	margin-top: 10px;
}

.accordion-toggle {
	color: black;
}

.feed-item, .nom-item {
	width: 99%;
